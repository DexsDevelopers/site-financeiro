    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "file_handlers": [
    {
      "action": "/seu_projeto/upload.php",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+painelfinanceiro",
      "url": "/seu_projeto/dashboard.php?action=%s"
    }
  ],
  "share_target": {
    "action": "/seu_projeto/dashboard.php",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "display_override": ["window-controls-overlay", "standalone"],
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate"
